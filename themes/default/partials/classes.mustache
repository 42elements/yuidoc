<h4>{{moduleName}}</h4>
<span class="foundat">Class definition found: <a href="../{{foundAt}}">`{{file}}:{{line}}`</a></span>
<div class="intro">{{{classDescription}}}</div>

<div id="classdocs">
    <ul>
        {{#if methods}}
        <li><a href="#methods">Methods</a></li>
        {{/if}}
        {{#if properties}}
        <li><a href="#props">Properties</a></li>
        {{/if}}
        {{#if attrs}}
        <li><a href="#attrs">Attributes</a></li>
        {{/if}}
        {{#if events}}
        <li><a href="#events">Events</a></li>
        {{/if}}
    </ul>
    <div>
        {{#if methods}}
            <div id="methods">
            {{#methods}}
                {{>method}}
            {{/methods}}
            </div>
        {{/if}}
        {{#if properties}}
            <div id="props">
            {{#properties}}
                {{>props}}
            {{/properties}}
            </div>
        {{/if}}
        {{#if attrs}}
            <div id="attrs">
            {{#attrs}}
                {{>attrs}}
            {{/attrs}}
            </div>
        {{/if}}
        {{#if events}}
            <div id="events">
            {{#events}}
                {{>events}}
            {{/events}}
            </div>
        {{/if}}
    </div>
</div>
